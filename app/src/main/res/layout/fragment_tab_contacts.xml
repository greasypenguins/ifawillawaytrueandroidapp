        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Phone:"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/contact_3_phone"
