<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="tech.thinkharder.ifawillawaytrueandroidapp.TabActivity$PlaceholderFragment">

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="@dimen/tab_label_top_margin"
        android:text="&lt;  Emergency Contacts    "
        android:textSize="@dimen/tab_label_height"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/contact_1_name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Name:"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/contact_1_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/contact_1_name" />

    <EditText
        android:id="@+id/contact_1_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Dr. Brimley"
        android:width="192dp"
        android:hint="Enter Contact"
        app:layout_constraintStart_toEndOf="@+id/contact_1_name_label"
        app:layout_constraintTop_toBottomOf="@+id/section_label" />

    <TextView
        android:id="@+id/contact_1_phone_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Phone:"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/contact_1_phone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/contact_1_phone" />

    <EditText
        android:id="@+id/contact_1_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:inputType="phone"
        android:text="785-867-5309"
        android:width="192dp"
        android:hint="Enter Phone Number"
        app:layout_constraintStart_toEndOf="@+id/contact_1_phone_label"
        app:layout_constraintTop_toBottomOf="@+id/contact_1_name" />

    <TextView
        android:id="@+id/contact_2_name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
